---
import Welcome from '../components/Welcome.astro';
import Layout from '../layouts/Layout.astro';

export const prerender = false;

await Astro.session?.set('user', {
	name: 'John Doe',
	email: 'john@doe.com',
});

const user = await Astro.session?.get('user') || { name: 'Guest' };

